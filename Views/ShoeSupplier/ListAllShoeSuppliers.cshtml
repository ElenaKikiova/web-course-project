@model CourseProject.ViewModels.ShoeSuppliers.AllShoeSuppliersViewModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListAllShoeSuppliers</title>
</head>
<body>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Supplier</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ShoeSuppliers) {
        <tr>
            <td>@item.Id</td>
            <td>@item.SupplierName</td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
